@font-face{
    font-family:'travel-icons';
    src: url(<%= asset_path('/assets/travelcons.eot') %>);
    src: url(<%= asset_path('/assets/travelcons.eot') %>) format('embedded-opentype'),
         url(<%= asset_path('/assets/travelcons.woff') %>) format('woff'),
         url(<%= asset_path('/assets/travelcons.ttf') %>) format('truetype');
    font-weight:normal;font-style:normal;}
.travelcon-subway:before,
.travelcon-walk:before,
.travelcon-bus:before,
.travelcon-rail:before,
.travelcon-boat:before{font-family:'travel-icons';font-style:normal;font-weight:normal;}
.travelcon-subway:before{content:"r";}
.travelcon-walk:before{content:":";}
.travelcon-bus:before{content:"b";}
.travelcon-rail:before{content:">";}
.travelcon-boat:before{content:"P"}

