- if alt_addresses.count > 0
  %h6= "Found #{alt_addresses.count} matching " + "address".pluralize(alt_addresses.count)
  %table.table.table-hover
    - alt_addresses.each_with_index do |place, index|
      %tr{:data => {:address_candidate => index, :latlon => [place[:lat], place[:lon]]}}
        %td
          = place[:formatted_address]

- else
  .alert.alert-info= "Nothing found at that address. Please try again"
