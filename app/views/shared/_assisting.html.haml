- Rails.logger.info assisting?
- Rails.logger.info session[:assisting].ai
- Rails.logger.info assisted_user.ai
- if assisting?
  .row-fluid
    .span12.alert.dropdown
      %div{style: 'text-align: center;'}
        %div{style: 'display: inline-block'}
          %a.dropdown-toggle{data: {toggle: 'dropdown', target: '#'}}
            %i.icon.icon-compass{style: 'font-size: 1.5em;'}
            = "Assisting #{assisted_user.email}"
            %b.caret
          %ul.dropdown-menu{role: 'menu'}
            %li
              = link_to desist_user_traveler_relationship_path(current_user, assisted_user) do
                %i.icon.icon-remove
                Stop assisting
