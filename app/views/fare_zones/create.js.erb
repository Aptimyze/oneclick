<% if remotipart_submitted? %>
  <% if @failure %>
    $("#farezoneUploadDialog .modal-flash").html("<%= @error_msgs %>").addClass("alert alert-danger fade-in");
  <% else %> 
    $("#farezoneUploadDialog .modal-flash").empty().removeClass("alert alert-danger fade-in");
    $('#farezoneUploadDialog').modal('toggle');
    $('#zoneFaresTable').empty();
    $('#zoneFaresTable').html("<%= j "#{render partial: 'services/paratransit_fares/zone_fare_form', locals: {zones:@zones, fares: @fares}}"%>");
  <% end %>
<% else %>
  if(<%=@failure%>)
    $("#farezoneUploadDialog .modal-flash").html("<%= @error_msgs %>").addClass("alert alert-danger fade-in");
<% end %>